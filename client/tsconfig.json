{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@models/*": [
        "../models/*"
      ],
      "@api/*": [
        "../server/src/api/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}